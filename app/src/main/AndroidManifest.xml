<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dhn.peanut">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".PeanutApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".ShotDetail.ShotDtailActivity"
            android:label="@string/title_activity_shot_dtail"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".PictrueDetail.PictureDetailActivity"
            android:label="@string/title_activity_picture_detail"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".login.LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>
